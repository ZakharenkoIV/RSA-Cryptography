<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextArea?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.InstructionandGenerator.GeneratorController"
            prefHeight="400.0" prefWidth="600.0">
    <content>
        <VBox alignment="CENTER" spacing="20">
        <Button text="generate" fx:id="gen" style="-fx-font-size: 16;-fx-background-radius: 10;-fx-font-weight: bold" maxWidth="250"/>
        <TextArea promptText="generated prime will appear here" fx:id="tx" style="-fx-border-radius: 15;-fx-border-color: black;-fx-font-size: 14" prefWidth="600" prefHeight="100" editable="false"/>
        </VBox>
    </content>
</DialogPane>
